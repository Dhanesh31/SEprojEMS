  <script>
        function exportTableToExcel(tableID, filename = '') {
          var downloadLink;
          var dataType = 'application/vnd.ms-excel';
          var tableSelect = document.getElementById(tableID);
          var tableHTML = tableSelect.outerHTML.replace(/ /g, '%20');

          // Specify file name
          filename = filename ? filename + '.xls' : 'excel_data.xls';

          // Create download link element
          downloadLink = document.createElement("a");

          document.body.appendChild(downloadLink);

          if (navigator.msSaveOrOpenBlob) {
            var blob = new Blob(['\ufeff', tableHTML], {
              type: dataType
            });
            navigator.msSaveOrOpenBlob(blob, filename);
          } else {
            // Create a link to the file
            downloadLink.href = 'data:' + dataType + ', ' + tableHTML;

            // Setting the file name
            downloadLink.download = filename;

            //triggering the function
            downloadLink.click();
          }
        }


        function populate(s1,s2,s3)
        {

            var results = JSON.parse(document.getElementById("results").value);
            var s1 = document.getElementById(s1);
            var s2 = document.getElementById(s2);
            var s3 = document.getElementById(s3);
            s3.innerHTML = "";
            var arr=[];

            var s4 = document.getElementById('faculty_id');
            s4.innerHTML = "";

            if (s1.value=='ALL' && s2.value=='ALL')
            {
              for(var i=0;i<results.length;i++)
              {
                arr.push(results[i].elective_id)
              }
            }
            else if(s1.value=='ALL' && s2.value!='ALL')
            {
              for(var i=0;i<results.length;i++)
              {
                if(results[i].elective_dept==s2.value)
                {
                  arr.push(results[i].elective_id);
                }
              }
            }
            else if(s1.value!='ALL' && s2.value=='ALL')
            {
              for(var i=0;i<results.length;i++)
              {
                if(results[i].elective_sem==s1.value)
                {
                  arr.push(results[i].elective_id);
                }
              }
            }
            else
            {
              if(s2.value=='CSE')
              {
                for(var i=0;i<results.length;i++)
                {
                  if(results[i].elective_dept=='CSE' && results[i].elective_sem==s1.value)
                  {
                    arr.push(results[i].elective_id);
                  }
                }
              }
              else if(s2.value=='ECE')
              {
                for(var i=0;i<results.length;i++)
                {
                  if(results[i].elective_dept=='ECE' && results[i].elective_sem==s1.value)
                  {
                    arr.push(results[i].elective_id);
                  }
                }
              }
              else if(s2.value=='EEE')
              {
                for(var i=0;i<results.length;i++)
                {
                  if(results[i].elective_dept=='EEE' && results[i].elective_sem==s1.value)
                  {
                    arr.push(results[i].elective_id);
                  }
                }
              }
              else if(s2.value=='CIE')
              {
                for(var i=0;i<results.length;i++)
                {
                  if(results[i].elective_dept=='CIE' && results[i].elective_sem==s1.value)
                  {
                    arr.push(results[i].elective_id);
                  }
                }
              }
              else if(s2.value=='MEC')
              {
                for(var i=0;i<results.length;i++)
                {
                  if(results[i].elective_dept=='MEC' && results[i].elective_sem==s1.value)
                  {
                    arr.push(results[i].elective_id);
                  }
                }
              }
            }
            arr.push('ALL');
            var optionArray=arr;
            optionArray = Array.from(new Set(optionArray));
            var newoption = document.createElement("option");
            newoption.value = "0";
            newoption.innerHTML = "--Select Elective ID--";
            s3.options.add(newoption);

            for(var i = 0;i<optionArray.length;i++)
            {
                newoption = document.createElement("option");
                newoption.value = optionArray[i];
                newoption.innerHTML = optionArray[i];
                s3.options.add(newoption);
            }

            newoption = document.createElement("option");
            newoption.value = "0";
            newoption.innerHTML = "--Select Faculty ID--";
            s4.options.add(newoption);

        }

        function populatefac(s1, s2, s3, s4)
        {

          var results = JSON.parse(document.getElementById("results").value);
          var s1 = document.getElementById(s1);
          var s2 = document.getElementById(s2);
          var s3 = document.getElementById(s3);
          var s4 = document.getElementById(s4);


          s2.innerHTML = "";
          var arr = [];
          var newoption = document.createElement("option");
          newoption.value = "0";
          newoption.innerHTML = "--Select Faculty ID--";
          s2.options.add(newoption);

          if (s1.value == "ALL") {
            if (s3.value == 'ALL') {
              if (s4.value == 'ALL') {
                for (var i = 0; i < results.length; i++) {
                  arr.push(results[i].faculty_id);
                }
              } else {
                for (var i = 0; i < results.length; i++) {
                  if (results[i].elective_dept == s4.value) {
                    arr.push(results[i].faculty_id);
                  }
                }
              }
            } else {
              if (s4.value == 'ALL') {
                for (var i = 0; i < results.length; i++) {
                  if (results[i].elective_sem == s3.value) {
                    arr.push(results[i].faculty_id);
                  }
                }
              } else {
                for (var i = 0; i < results.length; i++) {
                  if (results[i].elective_dept == s4.value && results[i].elective_sem == s3.value) {
                    arr.push(results[i].faculty_id);
                  }
                }
              }
            }
          } else {
            if (s3.value == 'ALL') {
              if (s4.value == 'ALL') {
                for (var i = 0; i < results.length; i++) {
                  if (results[i].elective_id == s1.value) {
                    arr.push(results[i].faculty_id);
                  }
                }
              } else {
                for (var i = 0; i < results.length; i++) {
                  if (results[i].elective_dept == s4.value && results[i].elective_id == s1.value) {
                    arr.push(results[i].faculty_id);
                  }
                }
              }
            } else {
              if (s4.value == 'ALL') {
                for (var i = 0; i < results.length; i++) {
                  if (results[i].elective_sem == s3.value && results[i].elective_id == s1.value) {
                    arr.push(results[i].faculty_id);
                  }
                }
              } else {
                for (var i = 0; i < results.length; i++) {
                  if (results[i].elective_dept == s4.value && results[i].elective_sem == s3.value && results[i].elective_id == s1.value) {
                    arr.push(results[i].faculty_id);
                  }
                }
              }
            }
          }

          var optionArray = arr;
          optionArray = Array.from(new Set(optionArray));

          for (var i = 0; i < optionArray.length; i++) {
            newoption = document.createElement("option");
            newoption.value = optionArray[i];
            newoption.innerHTML = optionArray[i];
            s2.options.add(newoption);
          }

          newoption = document.createElement("option");
          newoption.value = "ALL";
          newoption.innerHTML = "ALL";
          s2.options.add(newoption);

        }

  </script>

