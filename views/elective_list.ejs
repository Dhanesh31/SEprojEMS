            </div>
            <br><br><br>

            <button type="submit" id='filter' class="btn btn-primary" formaction="/filter_list">Filter</button>
            <button onclick="exportTableToExcel('customers', 'elective-data')" class="btn btn-primary" href="#">Export as Excel</button>
            <br>
            <% if(results_table.length> 0) { %>

                <table id="customers" style="margin-top:20px">
