            <input type = "text" name="results" id ="results" class="input-text" value="<%=results %>" style="display:None">

            <label class="col-md-4 control-label h5 required">Semester:</label>
            <div class="col-md-4">
              <select class="combo form-control input-sm select-hidden-accessible" aria-hidden="true" name="filter_sem" id="filter_sem" onchange="populate('filter_sem','filter_dept','elective_id')">
                <option value="0" selected>--Select Semester--</option>
                <option value="4">4</option>
                <option value="5">5</option>
                <option value="6">6</option>
                <option value="7">7</option>
                <option value="ALL">ALL</option>
              </select>
            </div>



            <label class="col-md-4 control-label h5 required"> Department:</label>
            <div class="col-md-4">
              <select class="combo form-control input-sm select-hidden-accessible" aria-hidden="true" name="filter_dept" id="filter_dept" onchange="populate('filter_sem','filter_dept','elective_id')">
                <option value="0"selected>--Select Department--</option>
                <option value="CSE">CSE</option>
                <option value="ECE">ECE</option>
                <option value="EEE">EEE</option>
                <option value="MEC">MECH</option>
                <option value="CIE">CIVIL</option>
